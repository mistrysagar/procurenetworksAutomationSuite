<?xml version="1.0" encoding="UTF-8"?>
<suite name="ProcureNetworks" verbose="10" parallel="tests"
	thread-count="1">

	<!-- Put IP of localhost/Grid Hub machine -->
	<parameter name="selenium.host" value="localhost" />

	<!-- Put Port of localhost RC/Grid Hub machine -->
	<parameter name="selenium.port" value="6666" />

	<!-- Put target browser name as below: 1) Firefox : firefox 2) chrome : 
		chrome 3) IExplorer : ie8 / ie9 -->
	<parameter name="selenium.browser" value="firefox" />

	<!-- @NOTE: 1) Un-comment <test> tag to run respective test. 2) Comment 
		<test> tag to unrun respective test. -->

	<parameter name="selenium.url"
		value="https://inv-pre.qa.procurenetworks.com/auth/login" />

	<!-- 1 -->
	<!-- Procure Network -->

	<test name="Verify Procure network Login">

		<classes>

			<class name="com.procurenetworks.verifications.ProcureNetworks">

				<methods>

					<include name="Search_Last_Created_Asset">
					</include>

				</methods>

			</class>

		</classes>

	</test>


</suite> 